    "tokens": "file:../tokens"
  },
  "devDependencies": {
    "@storybook/addon-storysource": "^4.0.12",
    "@storybook/react": "^4.1.4",
    "lorem-ipsum": "^1.0.6",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "story2sketch": "^1.4.0",
    "styled-components": "^4.1.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.1.0"
  }
