{
  "name": "@nulogy/components",
  "version": "0.2.5",
  "description": "Component library for the Nulogy Design System - http://nulogy.design",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "yarn run eslint",
    "eslint": "eslint --config ./.eslintrc './**/*.js'",
    "eslint:fix": "yarn run eslint --fix",
    "start": "start-storybook -p 8080",
    "build": "webpack --mode production --config ../webpack.config.js",
    "watch": "yarn build --watch",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "sketch": "story2sketch --url http://localhost:8080/iframe.html --output stories.asketch.json",
    "icons": "node scripts/collect-icon-svgs",
    "storyshots": "jest --testPathPattern='Storyshots.spec.js'",
    "storyshots:update": "yarn storyshots -u",
    "storyshots:visual:initial": "yarn build-storybook && rm -rf scripts/visual-storyshots/__image_snapshots__ && jest scripts/visual-storyshots/Storyshots.visual.spec.js",
    "storyshots:visual": "yarn build-storybook && jest scripts/visual-storyshots/Storyshots.visual.spec.js",
    "test": "jest && yarn run lint"
  },
  "license": "MIT",
  "author": "Nulogy <info@nulogy.com> (https://github.com/nulogy)",
  "homepage": "http://nulogy.design",
  "repository": "https://github.com/nulogy/design-system",
  "bugs": {
    "url": "https://github.com/nulogy/design-system/issues"
  },
  "keywords": [
    "nulogy",
    "design system",
    "NDS",
    "react",
    "components",
    "component library"
  ],
  "engines": {
    "node": "*"
  },
  "main": "dist/main.js",
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@nulogy/tokens": "file:../tokens",
    "@storybook/addon-storyshots": "4.1.11",
    "@storybook/addon-storyshots-puppeteer": "4.1.11",
    "babel-jest": "24.0.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-preset-react": "6.24.1",
    "eslint-config-airbnb": "17.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.0.0",
    "prop-types": "15.6.2",
    "react-test-renderer": "16.7.0"
  },
  "dependencies": {
    "@nulogy/tokens": "0.2.1",
    "@sambego/storybook-styles": "^1.0.0",
    "@storybook/addon-info": "^4.1.4",
    "@storybook/addon-storysource": "^4.0.12",
    "@storybook/react": "^4.1.4",
    "polished": "^2.3.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "story2sketch": "^1.4.0",
    "styled-components": "^4.1.3",
    "styled-system": "^3.2.1",
    "svg-sprite-loader": "^3.8.0",
    "svgi": "^1.1.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|sass)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@nulogy)"
    ],
    "testRegex": ".*spec\\.js$",
    "setupFiles": [
      "<rootDir>/spec/support/registerContext.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/spec/support/specHelper.js"
    ]
  }
}
