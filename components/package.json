  "bugs": {
    "url": "https://github.com/nulogy/design-system/issues"
  },
  "keywords": [
    "nulogy",
    "design system",
    "NDS",
    "react",
    "components",
    "component library"
  ],
  "engines": {
    "node": "*"
  },
  "main": "dist/main.js",
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-components": "^3.2.6"
  },
  "dependencies": {
    "@nulogy/tokens": "file:../tokens",
    "@storybook/addon-info": "^4.1.4",
    "facepaint": "^1.2.1",
