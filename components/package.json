  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "start-storybook -p 8080",
    "build": "webpack --mode production --config ../webpack.config.js",
    "watch": "yarn build --watch",
    "build-storybook": "build-storybook",
