{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outDir": "./built",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "rootDir": "./src",
    "types": []
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
